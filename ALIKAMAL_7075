    var arr = [1,2,5,8,0,234,3];
    var arr2 = [];

    function() multiply_by_n(n){
            for(let i = 0; i < arr.length; i++){
                arr2.push(arr[i] * n);
            }
            return arr2;
    }

    Array.prototype.mul_n = multiply_by_n(3);

    console.log(arr);

    arr2 = arr.mul_n();
    console.log(arr2);




